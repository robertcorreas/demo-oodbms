<Window x:Class="OODBMS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OODBMS"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="825">
    <StackPanel>
        <Button Content="Cadastrar de empregados" Click="CadastrarEmpregados_OnClick"/>
        <Button Content="Filtrar pro idade" Click="FiltrarIdade_Click"/>
        <Button Content="Carregar todos os empregados" Click="CarregarEmpregados_Click"/>
        <local:ListarEmpregados x:Name="ListaEmpregadosView" Height="500"></local:ListarEmpregados>
    </StackPanel>
</Window>
